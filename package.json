{
  "name": "zx-smart-lamp",
  "version": "1.0.0",
  "description": "react, react-router, hot-reload, sass.",
  "main": "index.js",
  "author": "Jack Pan",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node ./lib/index.js",
    "dev": "NODE_ENV=development webpack-dev-server",
    "build": "NODE_ENV=production webpack -p",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "setupFiles": [
      "<rootDir>/src/tests/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|styl|less|sass|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.jsx$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/__mock__/fileTransformer.js"
    }
  },
  "bin": {
    "simple-redux-app": "./bin/sra.js"
  },
  "preferGlobal": true,
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-wildcard": "^5.0.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.1.3",
    "jest-babel": "^1.0.1",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "less-plugin-npm-import": "^2.1.0",
    "node-sass": "^4.9.0",
    "prettier": "^1.13.7",
    "react-hot-loader": "3.0.0-beta.6",
    "redux-logger": "^3.0.6",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.8",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.9.3"
  },
  "dependencies": {
    "antd": "^3.6.5",
    "echarts": "^4.1.0",
    "echarts-for-react": "^2.0.13",
    "express": "^4.16.3",
    "express-http-proxy": "^1.2.0",
    "fs-extra": "^5.0.0",
    "isomorphic-fetch": "^2.2.1",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-flow-player": "^1.1.9",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "sockjs-client": "^1.1.5",
    "stompjs": "^2.3.3",
    "video.js": "^7.0.5",
    "videojs-flash": "^2.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pphanson/zx-smart-lamp.git"
  },
  "bugs": {
